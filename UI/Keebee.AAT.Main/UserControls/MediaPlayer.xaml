<UserControl x:Class="Keebee.AAT.Main.UserControls.MediaPlayer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Keebee.AAT.Main.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="360" d:DesignWidth="680">
    <Grid>
        <MediaElement x:Name="MediaElement" Margin="0" VerticalAlignment="Top" Height="360" Width="640" MediaEnded="MediaElement_MediaEnded" MediaOpened="MediaElement_MediaOpened"/>
    </Grid>
</UserControl>
